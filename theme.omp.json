{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<>\ueeb6</> @{{ .HostName }} ",
          "foreground": "#ff5959",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "<#cccccc>\uf1fa</> {{ .Path }} ",
          "foreground": "#cccccc",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "\uf09b ",
            "cache_duration": "none"
          },
          "template": "<#000></>{{ .HEAD }}<#000> </>",
          "foreground": "#1ddd33",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "\uf155",
          "foreground": "#1ddd33",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
